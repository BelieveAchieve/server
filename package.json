{
  "name": "upchieve-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main",
    "dev:autolint": "nodemon main --exec 'npm run lint && node'",
    "test": "nyc ava --verbose",
    "lint": "eslint '**/*.ts' --fix && eslint '*.js' '{controllers,models,router,services,seeds,worker}/**/*.js' --no-eslintrc --config ./.eslintrc-javascript.js --fix",
    "lint:prod": "eslint '**/*.ts' --max-warnings 0 && eslint '*.js' '{controllers,models,router,services,seeds,worker}/**/*.js' --no-eslintrc --config ./.eslintrc-javascript.js --max-warnings 0",
    "worker": "ts-node worker",
    "worker:dev": "ts-node-dev worker",
    "add-cron": "ts-node worker/add-cron"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 25,
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "^5.8.0",
    "@small-tech/express-ws": "^1.0.1",
    "async": "^1.5.0",
    "base64url": "^3.0.1",
    "bcrypt": "3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bull": "^3.13.0",
    "chalk": "^2.4.2",
    "cli-progress": "^3.0.0",
    "connect-busboy": "0.0.2",
    "connect-livereload": "^0.5.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.1",
    "cron": "^1.8.2",
    "crypto": "0.0.3",
    "csv-parse": "^4.4.6",
    "ejs": "^2.6.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.16.4",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.14.2",
    "jszip": "^3.2.2",
    "kerberos": "0.0.24",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.23",
    "mongodb-extended-json": "^1.11.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "newrelic": "^6.5.0",
    "nodemailer": "^2.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "prettier": "1.19.1",
    "prettier-config-standard": "^1.0.1",
    "q": "^1.4.1",
    "socket.io": "^2.2.0",
    "twilio": "^3.28.1",
    "ua-parser-js": "^0.7.21",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "@types/bull": "^3.12.2",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "ava": "^1.3.1",
    "eslint": "^6.8.0",
    "mongodb-extended-json": "^1.11.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "prettier-standard": "^9.1.1",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
